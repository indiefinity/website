    "build": "npm-run-all --parallel build:**"
  },
  "devDependencies": {
    "http-proxy-middleware": "^0.19.1",
    "markdown-magic": "^0.1.25",
    "netlify-lambda": "^1.1.1",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
