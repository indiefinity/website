{
  "name": "netlify-fauna",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "faunadb": "^2.5.2"
  },
  "scripts": {
    "docs": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "start": "npm-run-all --parallel checkForFaunaKey start:app start:server",
